	<context:annotation-config />
	<import resource="classpath:olms-spring.xml" />
	<context:component-scan base-package="com.training." />
	<bean id="userDao" class="com.training.daoimplementation.UserDAO" />
	<bean id="anonymousUser" class="com.training.daoimplementation.AnonymousUser" />
</beans>